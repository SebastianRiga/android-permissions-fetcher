{
  "name": "android-permissions-fetcher",
  "version": "1.0.0",
  "description": "Loads all available android permissions and their data from the official google page and returns them as a js object, formatted json or writes them to a file.",
  "main": "dist/api.js",
  "author": "Sebastian Riga",
  "readme": "README.md",
  "license": "MIT",
  "private": true,
  "keywords": [
    "api",
    "android",
    "permissions",
    "typescript"
  ],
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "coverage": "yarn clean:coverage && yarn test --coverage",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "clean:node_modules": "rimraf node_modules",
    "superclean": "yarn clean:dist && yarn clean:coverage && yarn clean:node_modules",
    "prepare": "yarn build",
    "prepublish": "yarn test && yarn lint",
    "version": "yarn format && git add --all src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@types/jest": "^26.0.23",
    "axios": "^0.21.1",
    "jest": "^26.6.3",
    "node-html-parser": "^3.2.0",
    "object-hash": "^2.1.1",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  },
  "devDependencies": {
    "@types/node": "^15.0.2",
    "@types/object-hash": "^2.1.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
